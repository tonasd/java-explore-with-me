		{
			"name": "Поставить лайк событию",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.variables.set(\"userId\", 1);\r",
							"pm.variables.set(\"eventId\", 2);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Ответ должен содержать код статуса 200 и данные в формате json\", function () {\r",
							"    pm.response.to.have.status(200);  \r",
							"    pm.response.to.be.withBody;\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"const target = pm.response.json();\r",
							"\r",
							"pm.test(\"Рейтинг должен содержать поля: participantId, eventId, isPositive\", function () {\r",
							"pm.expect(target).to.have.property('participantId');\r",
							"pm.expect(target).to.have.property('eventId');\r",
							"pm.expect(target).to.have.property('positive');\r",
							"});\r",
							"\r",
							"pm.test(\"Данные в ответе должны соответствовать данным в запросе\", function () {\r",
							"    pm.expect(target.participantId).equal(parseInt(pm.variables.get(\"userId\")), 'id пользователя оставившего запрос должно совпадать');\r",
							"    pm.expect(target.eventId).equal(parseInt(pm.variables.get(\"eventId\")), 'id события должно соответствовать eventId');\r",
							"    pm.expect(target.positive).equal(true, 'like это положительная оценка');\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
