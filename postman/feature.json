		{
			"name": "Получение топ 1 событий",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const main = async () => {\r",
							"    const api = new API(pm);\r",
							"\r",
							"    try {\r",
							"        api.put(\"/users/3/like/2\", null, \"Не могу лайкнуть, что то не так\", false);\r",
							"        api.put(\"/users/1/dislike/3\", null, \"Не могу дизлайкнуть, что то не так\", false);\r",
							"    } catch(err) {\r",
							"        console.error(\"Ошибка при подготовке тестовых данных.\", err);\r",
							"    }\r",
							"};\r",
							"\r",
							"const interval = setInterval(() => {}, 1000);\r",
							"\r",
							"setTimeout(async () => \r",
							"    {\r",
							"        try {\r",
							"            await main();\r",
							"        } catch (e) {\r",
							"            console.error(e);\r",
							"        } finally {\r",
							"            clearInterval(interval);\r",
							"        }\r",
							"    },  \r",
							"    100 \r",
							");"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Ответ должен содержать код статуса 200 и данные в формате json\", function () {\r",
							"    pm.response.to.have.status(200);  \r",
							"    pm.response.to.be.withBody;\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"const target = pm.response.json();\r",
							"\r",
							"pm.test(\"Топ должен состоять из списка длиной 1\", function() {\r",
							"    pm.expect(target.length, 'Список должен быть длиной 1').to.eql(1);\r",
							"});\r",
							"\r",
							"pm.test(\"Топ должен содержать поля: id, title, eventDate, likes, dislikes\", function () {\r",
							"pm.expect(target[0]).to.have.property('id');\r",
							"pm.expect(target[0]).to.have.property('title');\r",
							"pm.expect(target[0]).to.have.property('eventDate');\r",
							"pm.expect(target[0]).to.have.property('likes');\r",
							"pm.expect(target[0]).to.have.property('dislikes');\r",
							"});\r",
							"\r",
							"pm.test(\"Данные в ответе должны соответствовать значениям(предварительно подготовлено)\", function () {\r",
							"    pm.expect(target[0].id).equal(2, 'id самого популярного события на данный момент');\r",
							"    pm.expect(target[0].likes).equal(2, 'количество лайков должно быть 2');\r",
							"       pm.expect(target[0].dislikes).equal(1, 'количество дизлайков должно быть 1');\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
